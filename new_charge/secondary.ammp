# set the secondary structure for residue ires-jres ;
# variable ires is the residue ;
# variable stype is 1 2 3 (alpha,beta, alpha-left); 
#;

mov jres ires; add jres 1;
serial i0 ires ca;
jl i0 0 done: ;
serial i1 ires c;
serial i2 jres n;
jl i2 0 done: ;
serial i3 jres ca;
serial i4 jres c;
serial i5 jres o;

je stype 1 alpha: ;
je stype 2 beta: ; 
je stype 3 almin: ;
je 1 1 done: ;
alpha: ;
tset i0 i1 i2 i3 180.;
#tset i2 i3 i4 i5 -56.;
tset i2 i3 i4 i5 134.;
jes i3 pro.ca done: ;
tset i1 i2 i3 i4 -56;
je 1 1 done: ;
beta: ;
tset i0 i1 i2 i3 180.;
#tset i2 i3 i4 i5 127.;
tset i2 i3 i4 i5 309.;
jes i3 pro.ca done: ;
tset i1 i2 i3 i4 -115;
je 1 1 done: ;
almin: ;
tset i0 i1 i2 i3 180.;
#tset i2 i3 i4 i5 60.;
tset i2 i3 i4 i5 -127.0;
jes i3 pro.ca done: ;
tset i1 i2 i3 i4 56;
je 1 1 done: ;
done: ;
